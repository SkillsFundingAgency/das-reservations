@using static SFA.DAS.Reservations.Web.Extensions.ZenDeskLabelExtensions

@{
    var sectionId = "360002328100";
}

@if (Configuration["AuthType"].Equals("employer", StringComparison.CurrentCultureIgnoreCase))
{
    sectionId = "360001841359";
}


@Html.SetZenDeskLabels($"rsrv-{ViewBag.Title}")

<script type="text/javascript">
    window.zESettings = {
        webWidget: {
            contactForm: {
                attachments: false
            },
            chat: {
                menuOptions: {
                    emailTranscript: false
                }
            },
            helpCenter: { 
                filter: {
                  section: "@sectionId"
                }
            }
        }
    };
</script>
